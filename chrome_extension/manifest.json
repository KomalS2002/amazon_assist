{
    "manifest_version": 3,
    "name": "YouTube Product Identifier",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.jpeg",
        "48": "images/icon48.jpeg",
        "128": "images/icon128.jpeg"
      }
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "images/icon16.jpeg",
      "48": "images/icon48.jpeg",
      "128": "images/icon128.jpeg"
    }
  }
  